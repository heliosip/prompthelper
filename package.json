    "@supabase/supabase-js": "^2.39.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.3.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@types/chrome": "^0.0.244",
    "@types/date-fns": "^2.6.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
