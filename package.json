{
  "name": "prompthelper",
  "version": "1.0.0",
  "description": "AI Prompt Helper Extension",
  "type": "module",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist node_modules",
    "reinstall": "npm run clean && npm install",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:server": "node dist/server/index.js",
    "test": "npm run test:server && npm run test:client",
    "test:server": "ts-node test/server.test.ts",
    "test:client": "jest",
    "test:watch": "nodemon --exec ts-node test/server.test.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "create-dirs": "node scripts/create-directories.js",
    "generate-icons": "node scripts/generate-icons.js",
    "build:extension": "npm run generate-icons && vite build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.11",
    "@supabase/supabase-js": "^2.48.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "framer-motion": "^10.16.4",
    "fs-extra": "^11.2.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^5.0.2",
    "@types/chrome": "^0.0.258",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.199",
    "@types/node": "^22.10.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-transition-group": "^4.4.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "axios": "^1.7.9",
    "concurrently": "^9.1.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^11.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.31",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.3.3",
    "terser": "^5.37.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "keywords": [
    "express",
    "typescript",
    "server",
    "extension",
    "ai",
    "prompt",
    "helper"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
