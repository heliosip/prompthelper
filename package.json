{
  "name": "prompthelper",
  "version": "1.0.0",
  "description": "AI Prompt Helper Extension",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:popup": "vite build --config vite.popup.config.ts",
